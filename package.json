{
  "name": "mod-beastmen",
  "version": "0.0.5",
  "description": "Custom Beastmen Faction (taurox)",
  "keywords": [
    "totalwar",
    "rts",
    "mod",
    "modding",
    "lua"
  ],
  "main": "working_data/script/campaign/main_warhammer/mod/mk/index.lua",
  "watch": {
    "mod": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "lua",
      "quiet": false,
      "legacyWatch": true,
      "delay": 250
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "npm run watch",
    "test": "npm run test-mod && npm run test-battle",
    "test-mod": "cd working_data/script/campaign/main_warhammer && lua test/test.lua",
    "test-battle": "cd working_data/script/campaign/main_warhammer && lua test/test.battle.lua",
    "mod": "npfm pack --globs '**/*' --cwd 'working_data' npfm_generated.pack -d",
    "postmod": "npm run copy",
    "copy": "cp npfm_generated.pack ../data/!!!mk_beastmen_taurox_unlocker.pack && cp imgs/thumbnail512.png ../data/!!!mk_beastmen_taurox_unlocker.png",
    "log": "npm run logs",
    "logs": "ctail ../*.log ../*.txt"
  },
  "author": "mklabs",
  "license": "MIT",
  "devDependencies": {
    "ctail": "^1.5.6",
    "npm-watch": "^0.3.0"
  }
}
