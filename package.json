{
  "name": "mod-beastmen",
  "version": "0.0.4",
  "description": "Custom Beastmen Faction (taurox)",
  "keywords": [
    "totalwar",
    "rts",
    "mod",
    "modding",
    "lua"
  ],
  "main": "working_data/script/campaign/main_warhammer/mod/mk/index.lua",
  "watch": {
    "mod": "retail/data/mod.pack"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "npm run watch",
    "test": "npm run test-mod && npm run test-battle",
    "test-mod": "cd working_data/script/campaign/main_warhammer/mod && lua test/test.lua",
    "test-battle": "cd working_data/script/campaign/main_warhammer/mod && lua test/test.battle.lua",
    "mod": "cp retail/data/mod.pack ../data/!!mk_beastmen_children_of_chaos.pack && cp imgs/thumbnail512.png ../data/!!mk_beastmen_children_of_chaos.png",
    "log": "npm run logs",
    "logs": "ctail ../*.log ../mk_lua_mod_log.txt ../lua_mod_log.txt ../uimf_log.txt"
  },
  "author": "mklabs",
  "license": "MIT",
  "devDependencies": {
    "ctail": "^1.5.6",
    "npm-watch": "^0.3.0"
  }
}
